{
  "name": "model-framework",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/libs/model-framework/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/libs/model-framework",
        "main": "packages/libs/model-framework/src/index.ts",
        "tsConfig": "packages/libs/model-framework/tsconfig.lib.json",
        "assets": ["packages/libs/model-framework/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/libs/model-framework/**/*.ts",
          "packages/libs/model-framework/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/libs/model-framework/jest.config.ts"
      }
    }
  },
  "tags": []
}
