curl -X POST http://localhost:8716/openai/v1/chat/completions \
  -H "Content-Type: application/json" \
  -d '{
    "model": "deepscaler",
    "messages": [
      {"role": "system", "content": "You are a helpful assistant."},
      {"role": "user", "content": "Hello, how are you?"}
    ],
    "temperature": 0.7,
    "top_p": 0.9,
    "max_tokens": 100,
    "stream": true
  }'

  curl -X POST http://localhost:8716/openai/v1/completions \
  -H "Content-Type: application/json" \
  -d '{
    "model": "deepscaler",
    "prompt": "Write a short story about a robot",
    "temperature": 0.7,
    "max_tokens": 150,
    "top_p": 0.9,
    "frequency_penalty": 0.5,
    "presence_penalty": 0.5,
    "stream": true
  }'


  curl -X POST http://localhost:8716/openai/v1/embeddings \
  -H "Content-Type: application/json" \
  -d '{
    "model": "deepscaler",
    "input": "The quick brown fox jumps over the lazy dog",
    "encoding_format": "float"
  }'


  curl http://localhost:8716/openai/v1/models


  curl http://localhost:8716/openai/v1/models/deepscaler

  curl http://localhost:8716/openai/v1/models/tags

  curl http://localhost:8716/openai/v1/version

  
  